<html>
<center>
  <h1> fill the details below for signin </h1>

<%=form_for @user do |f| %>
<%= f.label :firstname %></br>
<%= f.text_field :first_name %></br>
 <%= f.label :lastname%></br>
<%= f.text_field :last_name %></br>
<%= f.label :email %></br>
<%= f.text_field :email %></br>
<%= f.label :password %></br>
<%= f.password_field :password %></br>

<%= f.submit :submit %>
<% end %>
</center>
</html>
